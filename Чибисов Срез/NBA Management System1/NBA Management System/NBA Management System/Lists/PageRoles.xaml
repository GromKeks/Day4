<Page x:Class="NBA_Management_System.Lists.PageRoles"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NBA_Management_System.Lists"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Вход">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
            <Button x:Name="btnGuest" Content="Админ" Height="40" Width="150" FontSize="17" Margin="0,0,0,20" Click="btnGuest_Click"/>
            <Button x:Name="btnManager" Content="Менеджер" Height="40" Width="150" FontSize="17" Click="btnManager_Click"/>
        </StackPanel>
    </Grid>
</Page>
